{
  "compilerOptions": {
    "paths": {
      "util/*": ["./javascript_pipeline/webpack_src/*"],
      "viewmodule": ["./viewmodule/webpack_src"],
      "profiles": ["./profiles/webpack_src"],
      "dataman": ["./api_lib/webpack_src"]
    },
    "target": "esnext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": ["*/webpack_src/**/*.ts", "*/webpack_src/**/*.d.ts"],
  "exclude": ["node_modules"]
}
