{
  "compilerOptions": {
    "paths": {
      "common": ["./javascript_pipeline/webpack_src/common.ts"],
      "viewmodule": ["./viewmodule/webpack_src/viewmodule.ts"],
      "api_lib": ["./api_lib/webpack_src/api_lib.ts"]
    },
    "target": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": ["*/webpack_src/**/*.ts", "*/webpack_src/**/*.d.ts"],
  "exclude": ["node_modules"]
}
